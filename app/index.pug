            #mini-price-link.container.mini-price
                .row
                    .col-md-12.section-heading.text-center
                        h2.single-animate.animate-press-1 Κατάλογος
                        |
                        .row
                            .col-md-8.col-md-offset-2.subtext.single-animate.animate-press-2
                                h3
                                    | Far far away, behind the word mountains, far from the countries Vokalia and Consonantia, there live the blind texts.
                |
                each row in products
                    div(class=`${row.serie}`).row.serie
                        .col-md-6= row.serieName
                        .col-md-6
                            .pull-right(class=`icon-${row.imgsrc}`)
                    |
                    div(class=`product_${row.productid}`).row
                        .col-md-3
                            .product-id(style='display:none')= row.productid
                            div(id=`main-product-images${row.productid}`).left
                            .left(class=`${row.productid}`,style='display:none;')
                                a.grouped_elements(data-fancybox-group=`gallery${row.productid}`, href=`${row.left.ahref}`)
                                    img.big.img-responsive(src=`${row.left.imgsrc}`, alt=`${row.left.alt}`)
                                ul
                                    each lister in row.left.ul
                                        li
                                            a.grouped_elements(data-fancybox-group=`gallery${row.productid}`, href=`${lister.ahref}`)
                                                img.small(src=`${lister.imgsrc}`, alt=`${lister.alt}`)
                        .col-md-4
                            .middle
                                h2.title
                                    strong= row.middle.model
                                    br
                                    = row.middle.title
                                h4.type= row.middle.type
                                label(for=`mramor${row.productid}`)
                                select(id=`mramor${row.productid}`,name='mramor').mramor
                                    - var selector = row.middle.select
                                    each sel in selector
                                        option(value=`${sel.value}`, data-description=`${sel.dataDescription}`, data-imagesrc=`${sel.dataImagesrc}`)= sel.name
                                p.contactus
                                    button.btn.btn-default.btn-sm(type='button' data-toggle='modal' data-target='#contactpopup' data-whatever=`Request for ${row.middle.model}`) Ρωτηστε για τιμη
                                    button.btn.btn-default.btn-sm(type='button' data-toggle='modal' data-target='#extraTCM' data-whatever=`Request for ${row.middle.model}`) Extra επιλογές


                            -var gallery = row.middle.gallery
                            //b= JSON.stringify(gallery)
                            each gal in gallery
                                .left(class=`${gal.galleryId}`, style='display:none;')
                                    a.grouped_elements(data-fancybox-group=`gallery${gal.galleryId}`, href=`${gal.ahref}`)
                                        img.big.img-responsive(src=`${gal.imgsrc}`, alt=`${gal.alt}`)
                                    +gallery(gal.galleryId, gal.li)

                        .col-md-5
                            .right
                                table.table.table-hover.tblattributes
                                    thead
                                        tr
                                            th
                                            -var productModel = row.middle.model
                                            each val in productModel
                                                th= val
                                    tbody
                                        tr
                                            td
                                                span.svg-icon.watt(data-toggle="tooltip" title="Ισχύς")
                                            -var capacity = row.table.Capacity
                                            each val in capacity
                                                td= val
                                        tr
                                            td
                                                span.svg-icon.m2(data-toggle="tooltip" title="Τετραγωνικά μέτρα")
                                            -var area = row.table.Area
                                            each val in area
                                                td= val
                                        //tr
                                        //    td Ρυθμιστής
                                        //    td= row.table.Regulator
                                        tr
                                            td
                                                span.svg-icon.dimansions(data-toggle="tooltip" title="Διαστάσεις")
                                            -var size = row.table.Size
                                            each val in size
                                                td= val
                                        tr
                                            td
                                                span.svg-icon.volt(data-toggle="tooltip" title="Τάση")
                                            -var voltage = row.table.Voltage
                                            each val in voltage
                                                td= val
                    |
